{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "Financial Transaction Shipment List",
    "description": "The message creates a financial transaction for a shipment",
    "examples": [
        {
            "request-header": {
                "json-web-token": "eyJraWQiOvxxxxxxxxxxxxxxxx",
                "message-id": "1a8eb33e-xxx-0242ac130003",
                "aws-user-name": "a8eb33e-xxx-0242ac130003",
                "organizations-id": 10003,
                "role-list": [
                    "ROLE_NAME1",
                    "ROLE_NAME2"
                ]
            },
            "filter-header": {
                "items": [
                    "shipmentfinancialtransactions_id",
                    "shipments_id",
                    "financialtransactions_type",
                    "currency_type",
                    "transactions_amount"
                ],
                "limit": 3,
                "offset": 0,
                "sort_asc": [
                    "shipmentfinancialtransactions_id"
                ],
                "sort_desc": [
                    "client_company_id"
                ],
                "eq": {
                    "organizations_id": 10036
                },
                "gt": {
                    "client_company_id": 10
                },
                "lt": {
                    "transactions_timestamp": "2020-12-01"
                }
            }
        }
    ],
    "type": "object",
    "required": [
        "request-header"
    ],
    "optional": [
        "filter-header"
    ],
    "properties": {
        "request-header": {
            "$ref": "https://gitlab.com/soteapps/messages/-/raw/master/schema-definitions-v1.json#/definitions/request-header"
        },
        "filter-header": {
            "$ref": "https://gitlab.com/soteapps/messages/-/raw/master/schema-definitions-v1.json#/definitions/filter-header"
        }
    },
    "additionalProperties": true
}