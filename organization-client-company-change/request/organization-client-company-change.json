{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "row-version": 1,
            "client-company-id": 33,
            "client-company-name": "Client Two"
        }
    ],
    "required": [
        "row-version",
        "client-company-id",
        "client-company-name"
    ],
    "properties": {
        "row-version": {
            "$id": "#/properties/row-version",
            "type": "integer",
            "title": "The row-version schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "client-company-id": {
            "$id": "#/properties/client-company-id",
            "type": "integer",
            "title": "The client-company-id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                33
            ]
        },
        "client-company-name": {
            "$id": "#/properties/client-company-name",
            "type": "string",
            "title": "The client-company-name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Client Two"
            ]
        }
    },
    "additionalProperties": true
}