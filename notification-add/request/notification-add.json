{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Email Notification Add",
	"description": "This business service adds notification details to the database and sends an email as well.",
	"examples": [
		{
			"request-header": {
				"json-web-token": "eyJraWQiOvxxxxxxxxxxxxxxxx",
				"message-id": "1a8eb33e-xxx-0242ac130003",
				"aws-user-name": "a8eb33e-xxx-0242ac130003",
				"organizations-id": 10003,
				"role-list": [
					"ROLE_NAME1",
					"ROLE_NAME2"
				]
			},
			"client-company-id": 100,
			"name": "David",
			"contact-value": "david.gofman@getsote.com",
			"comm-channel": "EMAIL"
		}
	],
	"type": "object",
	"required": [
		"request-header",
		"client-company-id",
		"name",
		"contact-value",
		"comm-channel"
	],
	"optional": [],
	"properties": {
		"request-header": {
			"$ref": "https://gitlab.com/soteapps/messages/-/raw/master/schema-definitions-v1.json#/definitions/request-header"
		},
		"client-company-id": {
			"$id": "#/properties/client-company-id",
			"type": "integer",
			"title": "This is the system id assigned to the organization client company"
		},
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"title": "The first name used for personalization"
		},
		"contact-value": {
			"$id": "#/properties/contact-value",
			"type": "string",
			"title": "The value used to send the communication to a specific person"
		},
		"comm-channel": {
			"$id": "#/properties/comm-channel",
			"type": "string",
			"title": "The Communication Channel",
			"description": "The communication method that will be use to send.",
			"default": "EMAIL",
			"enum": [
				"EMAIL"
			]
		}
	},
	"additionalProperties": true
}