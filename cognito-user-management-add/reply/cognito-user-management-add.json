{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "message-id",
    "message"
  ],
  "properties": {
    "message-id": {
      "type": "string",
      "default": "",
      "title": "The message-id Schema",
      "examples": [
        "test-message"
      ]
    },
    "message": {
      "type": "object",
      "default": {},
      "title": "The message Schema",
      "required": [
        "cognito_username",
        "enabled",
        "informational_user",
        "row_version",
        "user_status",
        "user_is_active",
        "usermanagement_id"
      ],
      "properties": {
        "cognito_username": {
          "type": "string",
          "default": "",
          "title": "The cognito_username Schema",
          "examples": [
            "653xxxxxxxxxxxxxxxx"
          ]
        },
        "enabled": {
          "type": "boolean",
          "default": false,
          "title": "The enabled Schema",
          "description": "This shows if user is active in cognito",
          "examples": [
            true
          ]
        },
        "informational_user": {
          "type": "boolean",
          "default": false,
          "title": "The informational_user Schema",
          "examples": [
            false
          ]
        },
        "row_version": {
          "type": "integer",
          "default": 0,
          "title": "The row_version Schema",
          "examples": [
            1
          ]
        },
        "user_status": {
          "type": "string",
          "default": "",
          "title": "The user_status Schema",
          "description": "The status of a newly created user",
          "examples": [
            "FORCE_CHANGE_PASSWORD"
          ]
        },
        "user_is_active": {
          "type": "boolean",
          "default": false,
          "title": "The user_is_active Schema",
          "description": "This shows if user is active in user-management database",
          "examples": [
            true
          ]
        },
        "usermanagement_id": {
          "type": "integer",
          "default": 0,
          "title": "The usermanagement_id Schema",
          "examples": [
            3000
          ]
        }
      },
      "examples": [{
        "cognito_username": "653xxxxxxxxxxxxxxxx",
        "enabled": true,
        "informational_user": false,
        "row_version": 1,
        "user_status": "FORCE_CHANGE_PASSWORD",
        "user_is_active": true,
        "usermanagement_id": 3000
      }]
    }
  },
  "examples": [{
    "message-id": "test-message",
    "message": {
      "cognito_username": "653xxxxxxxxxxxxxxxx",
      "enabled": true,
      "informational_user": false,
      "row_version": 1,
      "user_status": "FORCE_CHANGE_PASSWORD",
      "user_is_active": true,
      "usermanagement_id": 3000
    }
  }]
}