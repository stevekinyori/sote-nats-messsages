{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "json-web-token": "eyJraWQiOvxxxxxxxxxxxxxxxx",
            "message-id": "51ad84d4-XXXXXXX-6b2ce5e8",
            "aws-user-name": "d8b0e9d9-XXXXXXXX-f59910ef9d2b",
            "role-list": [
                "ROLE_NAME1",
                "ROLE_NAME2"
            ],
            "organizations-id": 1,
            "client-company-id": 1,
            "client-company-name": "My Enterprises Ltd",
            "suppliers-id": 1,
            "supplier-name": "Supplier Name",
            "bill-of-lading": "WEXXXXXXXX58",
            "port-of-entry": "Mombasa_Kenya",
            "port-of-origin": "SOKHNA__EGYPT",
            "country-destination": "KENYA",
            "shipment-port-of-entry-eta": "2020-12-26",
            "consignment-wgt": 17,
            "consignment-wgt-uom": "MT",
            "through-bill-of-lading": "TBL"
        }
    ],
    "required": [
        "json-web-token",
        "message-id",
        "aws-user-name",
        "role-list",
        "organizations-id",
        "client-company-id",
        "client-company-name"
        
    ],
    "optional": [
        "suppliers-id",
        "supplier-name",
        "bill-of-lading",
        "port-of-entry",
        "port-of-origin",
        "country-destination",
        "shipment-port-of-entry-eta",
        "consignment-wgt",
        "consignment-wgt-uom",
        "through-bill-of-lading"
    ],
    "properties": {
        "json-web-token": {
            "$id": "#/properties/json-web-token",
            "type": "string",
            "title": "The json-web-token schema",
            "description": "Authentication JWT.",
            "default": "",
            "examples": [
                "eyJraWQiOvxxxxxxxxxxxxxxxx"
            ]
        },
        "message-id": {
            "$id": "#/properties/message-id",
            "type": "string",
            "title": "The message-id schema",
            "description": "This is a unique identifier of each message.",
            "default": "",
            "examples": [
                "51ad84d4-XXXXXXX-6b2ce5e8"
            ]
        },
        "aws-user-name": {
            "$id": "#/properties/aws-user-name",
            "type": "string",
            "title": "The aws-user-name schema",
            "description": "The Cognito username for the person who is requesting the record creation.",
            "default": "",
            "examples": [
                "d8b0e9d9-XXXXXXXX-f59910ef9d2b"
            ]
        },
        "role-list": {
            "$id": "#/properties/role-list",
            "type": "array",
            "title": "The role-list schema",
            "description": "The roles that the user requesting the record creation has been assigned.",
            "default": [],
            "examples": [
                [
                    "ROLE_NAME1",
                    "ROLE_NAME2"
                ]
            ],
            "additionalItems": true,
            "items": {
                "$id": "#/properties/role-list/items",
                "anyOf": [
                    {
                        "$id": "#/properties/role-list/items/anyOf/0",
                        "type": "string",
                        "title": "The first anyOf schema",
                        "description": "Instance of user role list.",
                        "default": "",
                        "examples": [
                            "ROLE_NAME1",
                            "ROLE_NAME2"
                        ]
                    }
                ]
            }
        },
        "organizations-id": {
            "$id": "#/properties/organizations-id",
            "type": "integer",
            "title": "The organizations-id schema",
            "description": "This is a system generated id that is used to restrict access to customers data.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "client-company-id": {
            "$id": "#/properties/client-company-id",
            "type": "integer",
            "title": "The client-company-id schema",
            "description": "This is a system generated id that is used identify a customer.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "client-company-name": {
            "$id": "#/properties/client-company-name",
            "type": "string",
            "title": "The client-company-name schema",
            "description": "This is the name the customer uses to identify itself to the public.",
            "default": "",
            "examples": [
                "My Enterprises Ltd"
            ]
        },
        "suppliers-id": {
            "$id": "#/properties/suppliers-id",
            "type": "integer",
            "title": "The suppliers-id schema",
            "description": "This is a system generated id that is used identify a company supplier. Is required when supplier-name is supplied.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "supplier-name": {
            "$id": "#/properties/supplier-name",
            "type": "string",
            "title": "The supplier-name schema",
            "description": "The name that is used by the supplier. Is required when supplier-id is supplied",
            "default": "",
            "examples": [
                "Supplier Name"
            ]
        },
        "bill-of-lading": {
            "$id": "#/properties/bill-of-lading",
            "type": "string",
            "title": "The bill-of-lading schema",
            "description": "This is an externally provided value for identification of the shipment.",
            "default": "",
            "examples": [
                "WEXXXXXXXX58"
            ]
        },
        "port-of-entry": {
            "$id": "#/properties/port-of-entry",
            "type": "string",
            "title": "The port-of-entry schema",
            "description": "This is the name of the port that the goods will enter.",
            "default": "",
            "examples": [
                "Mombasa_Kenya"
            ]
        },
        "port-of-origin": {
            "$id": "#/properties/port-of-origin",
            "type": "string",
            "title": "The port-of-origin schema",
            "description": "This is the name of the port that the goods left.",
            "default": "",
            "examples": [
                "SOKHNA__EGYPT"
            ]
        },
        "country-destination": {
            "$id": "#/properties/country-destination",
            "type": "string",
            "title": "The country-destination schema",
            "description": "This is the name of the country where the goods will be delivered.",
            "default": "",
            "examples": [
                "KENYA"
            ]
        },
        "shipment-port-of-entry-eta": {
            "$id": "#/properties/shipment-port-of-entry-eta",
            "type": "string",
            "title": "The shipment-port-of-entry-eta schema",
            "description": "Approximately when the cargo will arrive at the port of entry.",
            "default": "",
            "examples": [
                "2020-12-26"
            ]
        },
        "consignment-wgt": {
            "$id": "#/properties/consignment-wgt",
            "type": "integer",
            "title": "The consignment-wgt schema",
            "description": "This is the total gross weight for the shipment, including the product and packaging.",
            "default": 0,
            "examples": [
                17
            ]
        },
        "consignment-wgt-uom": {
            "$id": "#/properties/consignment-wgt-uom",
            "type": "string",
            "title": "The consignment-wgt-uom schema",
            "description": "What measurement is used for the weight supplied.",
            "default": "",
            "examples": [
                "MT"
            ]
        },
        "through-bill-of-lading": {
            "$id": "#/properties/through-bill-of-lading",
            "type": "string",
            "title": "The through-bill-of-lading schema",
            "description": "The external identifier for paper working covering empty containers.",
            "default": "",
            "examples": [
                "TBL"
            ]
        }
    },
    "additionalProperties": true
}