        "notes_note": {
            "type": "string",
            "default": "",
            "title": "The notes_note Schema",
            "examples": [
                "Update note test"
            ]
        },
        "syntax_type": {
            "type": "string",
            "default": "",
            "title": "The syntax_type Schema",
            "examples": [
                "TEXT"
            ]
        }
    },
    "examples": [{
        "notes_id": 2,
        "notes_note": "Update note test",
        "syntax_type": "TEXT"
    }]
}