{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "message-id",
    "message"
  ],
  "properties": {
    "message-id": {
      "type": "string",
      "default": "",
      "title": "The message-id Schema",
      "examples": [
        "test-message"
      ]
    },
    "message": {
      "type": "object",
      "default": {},
      "title": "The message Schema",
      "required": [
        "system_name",
        "display_name",
        "listofvalues_note",
        "language_code",
        "language_name",
        "created_by_date",
        "created_by_requestor_username",
        "updated_by_date",
        "updated_by_requestor_username",
        "value_is_active"
      ],
      "properties": {
        "system_name": {
          "type": "string",
          "default": "",
          "title": "The system_name Schema",
          "examples": [
            "TEST_ONE"
          ]
        },
        "display_name": {
          "type": "string",
          "default": "",
          "title": "The display_name Schema",
          "examples": [
            "Test One Edit"
          ]
        },
        "listofvalues_note": {
          "type": "string",
          "default": "",
          "title": "The listofvalues_note Schema",
          "examples": [
            "This is a test note edit"
          ]
        },
        "language_code": {
          "type": "string",
          "default": "",
          "title": "The language_code Schema",
          "examples": [
            "ENG"
          ]
        },
        "language_name": {
          "type": "string",
          "default": "",
          "title": "The language_name Schema",
          "examples": [
            "ENGLISH"
          ]
        },
        "created_by_date": {
          "type": "string",
          "default": "",
          "title": "The created_by_date Schema",
          "examples": [
            "2022-06-26"
          ]
        },
        "created_by_requestor_username": {
          "type": "string",
          "default": "",
          "title": "The created_by_requestor_username Schema",
          "examples": [
            "sote-test"
          ]
        },
        "updated_by_date": {
          "type": "string",
          "default": "",
          "title": "The updated_by_date Schema",
          "examples": [
            "2022-06-26"
          ]
        },
        "updated_by_requestor_username": {
          "type": "string",
          "default": "",
          "title": "The updated_by_requestor_username Schema",
          "examples": [
            "sote-test"
          ]
        },
        "value_is_active": {
          "type": "boolean",
          "default": false,
          "title": "The value_is_active Schema",
          "examples": [
            true
          ]
        }
      },
      "examples": [{
        "system_name": "TEST_ONE",
        "display_name": "Test One Edit",
        "listofvalues_note": "This is a test note edit",
        "language_code": "ENG",
        "language_name": "ENGLISH",
        "created_by_date": "2022-06-26",
        "created_by_requestor_username": "sote-test",
        "updated_by_date": "2022-06-26",
        "updated_by_requestor_username": "sote-test",
        "value_is_active": true
      }]
    }
  },
  "examples": [{
    "message-id": "test-message",
    "message": {
      "system_name": "TEST_ONE",
      "display_name": "Test One Edit",
      "listofvalues_note": "This is a test note edit",
      "language_code": "ENG",
      "language_name": "ENGLISH",
      "created_by_date": "2022-06-26",
      "created_by_requestor_username": "sote-test",
      "updated_by_date": "2022-06-26",
      "updated_by_requestor_username": "sote-test",
      "value_is_active": true
    }
  }]
}