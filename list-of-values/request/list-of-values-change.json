{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "list-of-values-type",
        "system-name",
        "display-name",
        "list-of-values-note",
        "language-code",
        "language-name",
        "value-is-active"
    ],
    "properties": {
        "list-of-values-type": {
            "type": "string",
            "default": "",
            "title": "The list-of-values-type Schema",
            "examples": [
                "ADD_TEST"
            ]
        },
        "system-name": {
            "type": "string",
            "default": "",
            "title": "The system-name Schema",
            "examples": [
                "TEST_ONE"
            ]
        },
        "display-name": {
            "type": "string",
            "default": "",
            "title": "The display-name Schema",
            "examples": [
                "Test One Edit"
            ]
        },
        "list-of-values-note": {
            "type": "string",
            "default": "",
            "title": "The list-of-values-note Schema",
            "examples": [
                "This is a test note edit"
            ]
        },
        "language-code": {
            "type": "string",
            "default": "",
            "title": "The language-code Schema",
            "examples": [
                "eng"
            ]
        },
        "language-name": {
            "type": "string",
            "default": "",
            "title": "The language-name Schema",
            "examples": [
                "English"
            ]
        },
        "value-is-active": {
            "type": "boolean",
            "default": false,
            "title": "The value-is-active Schema",
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "list-of-values-type": "ADD_TEST",
        "system-name": "TEST_ONE",
        "display-name": "Test One Edit",
        "list-of-values-note": "This is a test note edit",
        "language-code": "eng",
        "language-name": "English",
        "value-is-active": true
    }]
}