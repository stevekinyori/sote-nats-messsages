{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "Email Notification List",
	"description": "This business service adds notification details to the database and sends an email as well.",
	"examples": [
		{
			"request-header": {
				"json-web-token": "eyJraWQiOvxxxxxxxxxxxxxxxx",
				"message-id": "1a8eb33e-xxx-0242ac130003",
				"aws-user-name": "a8eb33e-xxx-0242ac130003",
				"organizations-id": 10003,
				"role-list": [
					"ROLE_NAME1",
					"ROLE_NAME2"
				]
			},
			"filter-header": {
				"items": [
					"clientcompanycontacts_id",
					"client_company_id",
					"communication_type",
					"contact_value",
					"name"
				],
				"limit": 3,
				"offset": 0,
				"sort_asc": [
					"name",
					"contact_value"
				],
				"sort_desc": [
					"client_company_id"
				],
				"eq": {
					"communication_type": "EMAIL"
				},
				"gt": {
					"clientcompanycontacts_id": 1
				},
				"lt": {
					"created_by_date": "2020-12-01"
				}
			}
		}
	],
	"type": "object",
	"required": ["request-header"],
	"optional": ["filter-header"],
	"properties": {
		"request-header": {
			"$ref": "https://gitlab.com/soteapps/messages/-/raw/master/schema-definitions-v1.json#/definitions/request-header"
		},
		"filter-header": {
			"$ref": "https://gitlab.com/soteapps/messages/-/raw/master/schema-definitions-v1.json#/definitions/filter-header"
		}
	},
	"additionalProperties": true
}