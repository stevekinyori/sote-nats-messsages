{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "message-id": "51ad84d4-XXXXXXX-6b2ce5e8",
            "message": {
                "STATUS_1": 9,
                "STATUS_2": 13
            }
        }
    ],
    "required": [
        "message-id",
        "message"
    ],
    "properties": {
        "message-id": {
            "$id": "#/properties/message-id",
            "type": "string",
            "title": "The message-id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "51ad84d4-XXXXXXX-6b2ce5e8"
            ]
        },
        "message": {
            "$id": "#/properties/message",
            "type": "object",
            "title": "The message schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "STATUS_1": 9,
                    "STATUS_2": 13
                }
            ],
            "required": [
                "STATUS_1",
                "STATUS_2"
            ],
            "properties": {
                "STATUS_1": {
                    "$id": "#/properties/message/properties/STATUS_1",
                    "type": "integer",
                    "title": "The STATUS_1 schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        9
                    ]
                },
                "STATUS_2": {
                    "$id": "#/properties/message/properties/STATUS_2",
                    "type": "integer",
                    "title": "The STATUS_2 schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": 0,
                    "examples": [
                        13
                    ]
                }
            },
            "additionalProperties": true
        }
    },
    "additionalProperties": true
}