{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "message-id",
    "message"
  ],
  "properties": {
    "message-id": {
      "type": "string",
      "default": "",
      "title": "The message-id Schema",
      "examples": [
        "test-message"
      ]
    },
    "message": {
      "type": "object",
      "default": {},
      "title": "The message Schema",
      "required": [
        "informational_user",
        "row_version",
        "user_is_active",
        "usermanagement_id"
      ],
      "properties": {
        "informational_user": {
          "type": "boolean",
          "default": false,
          "title": "The informational_user Schema",
          "examples": [
            true
          ]
        },
        "row_version": {
          "type": "integer",
          "default": 0,
          "title": "The row_version Schema",
          "examples": [
            19
          ]
        },
        "user_is_active": {
          "type": "boolean",
          "default": false,
          "title": "The user_is_active Schema",
          "examples": [
            false
          ]
        },
        "usermanagement_id": {
          "type": "integer",
          "default": 0,
          "title": "The usermanagement_id Schema",
          "examples": [
            3
          ]
        }
      },
      "examples": [{
        "informational_user": true,
        "row_version": 19,
        "user_is_active": false,
        "usermanagement_id": 3
      }]
    }
  },
  "examples": [{
    "message-id": "test-message",
    "message": {
      "informational_user": true,
      "row_version": 19,
      "user_is_active": false,
      "usermanagement_id": 3
    }
  }]
}