{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "json-web-token": "eyJraWQiOvxxxxxxxxxxxxxxxx",
            "message-id": "51ad84d4-XXXXXXX-6b2ce5e8",
            "aws-user-name": "d8b0e9d9-XXXXXXXX-f59910ef9d2b",
            "role-list": [
                "ROLE_NAME1",
                "ROLE_NAME2"
            ],
            "organizations-id": 1,
            "shipments-id": 1,
            "cargo-parent-type": "CONTAINER",
            "cargo-type": "Sand",
            "containers-id": 1,
            "cargo-qty": 400,
            "cargo-qty-uom": "bags",
            "cargo-gross-wgt": 10130,
            "cargo-gross-uom": "KGS"
        }
    ],
    "required": [
        "json-web-token",
        "message-id",
        "aws-user-name",
        "role-list",
        "organizations-id",
        "shipments-id",
        "cargo-parent-type",
        "cargo-type"
    ],
    "optional": [
        "containers-id",
        "cargo-qty",
        "cargo-qty-uom",
        "cargo-gross-wgt",
        "cargo-gross-uom"
    ],
    "properties": {
        "json-web-token": {
            "$id": "#/properties/json-web-token",
            "type": "string",
            "title": "The json-web-token schema",
            "description": "Authentication JWT.",
            "default": "",
            "examples": [
                "eyJraWQiOvxxxxxxxxxxxxxxxx"
            ]
        },
        "message-id": {
            "$id": "#/properties/message-id",
            "type": "string",
            "title": "The message-id schema",
            "description": "This is a unique identifier of each message.",
            "default": "",
            "examples": [
                "51ad84d4-XXXXXXX-6b2ce5e8"
            ]
        },
        "aws-user-name": {
            "$id": "#/properties/aws-user-name",
            "type": "string",
            "title": "The aws-user-name schema",
            "description": "The Cognito username for the person who is requesting the record creation.",
            "default": "",
            "examples": [
                "d8b0e9d9-XXXXXXXX-f59910ef9d2b"
            ]
        },
        "role-list": {
            "$id": "#/properties/role-list",
            "type": "array",
            "title": "The role-list schema",
            "description": "The roles that the user requesting the record creation has been assigned.",
            "default": [],
            "examples": [
                [
                    "ROLE_NAME1",
                    "ROLE_NAME2"
                ]
            ],
            "additionalItems": true,
            "items": {
                "$id": "#/properties/role-list/items",
                "anyOf": [
                    {
                        "$id": "#/properties/role-list/items/anyOf/0",
                        "type": "string",
                        "title": "The first anyOf schema",
                        "description": "Instance of user role list.",
                        "default": "",
                        "examples": [
                            "ROLE_NAME1",
                            "ROLE_NAME2"
                        ]
                    }
                ]
            }
        },
        "organizations-id": {
            "$id": "#/properties/organizations-id",
            "type": "integer",
            "title": "The organizations-id schema",
            "description": "This is a system generated id that is used to restrict access to customers data.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "shipments-id": {
            "$id": "#/properties/shipments-id",
            "type": "integer",
            "title": "The shipments-id schema",
            "description": "This is the system generated id for the shipment.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "cargo-parent-type": {
            "$id": "#/properties/cargo-parent-type",
            "type": "string",
            "title": "The cargo-parent-type schema",
            "description": "Who is the parent of this cargo, shipment or container",
            "default": "",
            "examples": [
                "CONTAINER",
                "SHIPMENT"
            ]
        },
        "cargo-type": {
            "$id": "#/properties/cargo-type",
            "type": "string",
            "title": "The cargo-type schema",
            "description": "A general name of what good or product is being conveyed.",
            "default": "",
            "examples": [
                "Sand"
            ]
        },
        "containers-id": {
            "$id": "#/properties/containers-id",
            "type": "integer",
            "title": "The containers-id schema",
            "description": "This is a system generated id for a container assigned to a container. Required if cargo-parent-type is CONTAINER.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "cargo-qty": {
            "$id": "#/properties/cargo-qty",
            "type": "integer",
            "title": "The cargo-qty schema",
            "description": "The total.",
            "default": 0,
            "examples": [
                400
            ]
        },
        "cargo-qty-uom": {
            "$id": "#/properties/cargo-qty-uom",
            "type": "string",
            "title": "The cargo-qty-uom schema",
            "description": "What measurement is used for the quantity supplied.",
            "default": "",
            "examples": [
                "bags"
            ]
        },
        "cargo-gross-wgt": {
            "$id": "#/properties/cargo-gross-wgt",
            "type": "integer",
            "title": "The cargo-gross-wgt schema",
            "description": "The gross weight of the cargo.",
            "default": 0,
            "examples": [
                10130
            ]
        },
        "cargo-gross-uom": {
            "$id": "#/properties/cargo-gross-uom",
            "type": "string",
            "title": "The cargo-gross-uom schema",
            "description": "What measurement is used for the weight supplied.",
            "default": "",
            "examples": [
                "KGS"
            ]
        }
    },
    "additionalProperties": true
}