                  "IN"
                ]
              },
              "value": {
                "title": "The value Schema",
                "type": "array",
                "default": [],
                "items": {
                  "title": "A Schema",
                  "type": "string",
                  "examples": [
                    "2",
                    "1",
                    "1000",
                    "10772"
                  ]
                },
                "examples": [
                  [
                    "2",
                    "1",
                    "1000",
                    "10772"
                  ]
                ]
              }
            },
            "examples": [
              {
                "field-name": "trips-id",
                "operator": "IN",
                "value": [
                  "2",
                  "1",
                  "1000",
                  "10772"
                ]
              }
            ]
          },
          "examples": [
            [
              {
                "field-name": "trips-id",
                "operator": "IN",
                "value": [
                  "2",
                  "1",
                  "1000",
                  "10772"
                ]
