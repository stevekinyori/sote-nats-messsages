{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "message-id": "test",
      "message": {
        "success": true
      }
    }
  ],
  "required": [
    "message-id",
    "message"
  ],
  "properties": {
    "message-id": {
      "$id": "#/properties/message-id",
      "default": "",
      "description": "This is used to uniquely identify devices.",
      "examples": [
        "test"
      ],
      "title": "The message-id schema",
      "type": "string"
    },
    "message": {
      "$id": "#/properties/message",
      "type": "object",
      "title": "The message schema",
      "description": "An explanation about the purpose of this instance.",
      "default": {},
      "examples": [
        {
          "success": true
        }
      ],
      "required": [
        "success"
      ],
      "properties": {
        "success": {
          "$id": "#/properties/message/properties/success",
          "default": false,
          "description": "Evaluates to true if document is successfully deleted, else false",
          "examples": [
            true
          ],
          "title": "The success schema",
          "type": "boolean"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}