{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "message-id": "test",
      "message": {
        "documents_id": 1
      }
    }
  ],
  "required": [
    "message-id",
    "message"
  ],
  "properties": {
    "message-id": {
      "$id": "#/properties/message-id",
      "default": "",
      "description": "This is used to uniquely identify devices.",
      "examples": [
        "test"
      ],
      "title": "The message-id schema",
      "type": "string"
    },
    "message": {
      "$id": "#/properties/message",
      "type": "object",
      "title": "The message schema",
      "description": "An explanation about the purpose of this instance.",
      "default": {},
      "examples": [
        {
          "documents_id": 1
        }
      ],
      "required": [
        "documents_id"
      ],
      "properties": {
        "documents_id": {
          "$id": "#/properties/message/properties/documents_id",
          "default": 0,
          "description": "This is a system generated id that is used identify a document.",
          "examples": [
            1
          ],
          "title": "The documents_id schema",
          "type": "integer"
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}