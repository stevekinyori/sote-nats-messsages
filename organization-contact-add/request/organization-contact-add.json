{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "organizations-id",
        "client-company-id",
        "communication-type",
        "contact-value",
        "contact-name"
    ],
    "properties": {
        "organizations-id": {
            "type": "integer",
            "default": 0,
            "title": "The organizations-id Schema",
            "examples": [
                1
            ]
        },
        "client-company-id": {
            "type": "integer",
            "default": 0,
            "title": "The client-company-id Schema",
            "examples": [
                1
            ]
        },
        "communication-type": {
            "type": "string",
            "default": "",
            "title": "The communication-type Schema",
            "examples": [
                "email"
            ]
        },
        "contact-value": {
            "type": "string",
            "default": "",
            "title": "The contact-value Schema",
            "examples": [
                "test1@test.com"
            ]
        },
        "contact-name": {
            "type": "string",
            "default": "",
            "title": "The contact-name Schema",
            "examples": [
                "Test One"
            ]
        }
    },
    "examples": [{
        "organizations-id": 1,
        "client-company-id": 1,
        "communication-type": "email",
        "contact-value": "test1@test.com",
        "contact-name": "Test One"
    }]
}