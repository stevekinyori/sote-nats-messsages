{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "message-id": "test-list",
            "message": {
                "enabled": true,
                "user-status": "FORCE_CHANGE_PASSWORD",
                "cognito-username": "a46baxxxxxxxxxxxxxxxxxxxxxx"
            }
        }
    ],
    "required": [
        "message-id",
        "message"
    ],
    "properties": {
        "message-id": {
            "$id": "#/properties/message-id",
            "type": "string",
            "title": "The message-id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "test-list"
            ]
        },
        "message": {
            "$id": "#/properties/message",
            "type": "object",
            "title": "The message schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "enabled": true,
                    "user-status": "FORCE_CHANGE_PASSWORD",
                    "cognito-username": "a46baxxxxxxxxxxxxxxxxxxxxxx"
                }
            ],
            "required": [
                "enabled",
                "user-status",
                "cognito-username"
            ],
            "properties": {
                "enabled": {
                    "$id": "#/properties/message/properties/enabled",
                    "type": "boolean",
                    "title": "The enabled schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": false,
                    "examples": [
                        true
                    ]
                },
                "user-status": {
                    "$id": "#/properties/message/properties/user-status",
                    "type": "string",
                    "title": "The user-status schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "FORCE_CHANGE_PASSWORD"
                    ]
                },
                "cognito-username": {
                    "$id": "#/properties/message/properties/cognito-username",
                    "type": "string",
                    "title": "The cognito-username schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "a46baxxxxxxxxxxxxxxxxxxxxxx"
                    ]
                }
            },
            "additionalProperties": true
        }
    },
    "additionalProperties": true
}