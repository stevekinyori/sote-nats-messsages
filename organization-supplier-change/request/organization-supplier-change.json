{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "organizations-id": 1,
            "client-company-id": 1,
            "suppliers-id": 1,
            "supplier-name": "Supplier Two"
        }
    ],
    "required": [
        "organizations-id",
        "client-company-id",
        "suppliers-id",
        "supplier-name"
    ],
    "properties": {
        "organizations-id": {
            "$id": "#/properties/organizations-id",
            "type": "integer",
            "title": "The organizations-id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "client-company-id": {
            "$id": "#/properties/client-company-id",
            "type": "integer",
            "title": "The client-company-id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "suppliers-id": {
            "$id": "#/properties/suppliers-id",
            "type": "integer",
            "title": "The suppliers-id schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                1
            ]
        },
        "supplier-name": {
            "$id": "#/properties/supplier-name",
            "type": "string",
            "title": "The supplier-name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Supplier Two"
            ]
        }
    },
    "additionalProperties": true
}