{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "organizations-id",
    "user-management-id",
    "row-version",
    "cognito-username required if informational-user IS FALSE",
    "informational-user"
  ],
  "properties": {
    "organizations-id": {
      "type": "integer",
      "default": 0,
      "title": "The organizations-id Schema",
      "examples": [
        1
      ]
    },
    "user-management-id": {
      "type": "integer",
      "default": 0,
      "title": "The user-management-id Schema",
      "examples": [
        3
      ]
    },
    "row-version": {
      "type": "integer",
      "default": 0,
      "title": "The row-version Schema",
      "examples": [
        2
      ]
    },
    "cognito-username": {
      "type": "string",
      "default": "",
      "title": "The cognito-username Schema",
      "examples": [
        "f9bxxxxxxxxxxxx"
      ]
    },
    "informational-user": {
      "type": "boolean",
      "default": false,
      "title": "The informational-user Schema",
      "examples": [
        false
      ]
    }
  },
  "examples": [{
    "organizations-id": 1,
    "user-management-id": 3,
    "row-version": 2,
    "cognito-username": "f9bxxxxxxxxxxxx",
    "informational-user": false
  }]
}