{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "record-limit-count": 0,
            "filter": {
                "email": "test@email.com"
            },
            "pagination-token": "CAISoAIXXXXXX5AQgDEvQBANg6kXXXXXXXXX"
        }
    ],
    "required": [
        "record-limit-count"
    ],
    "properties": {
        "record-limit-count": {
            "$id": "#/properties/record-limit-count",
            "type": "integer",
            "title": "The record-limit-count schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                0
            ]
        },
        "filter": {
            "$id": "#/properties/filter",
            "type": "object",
            "title": "The filter schema",
            "description": "An explanation about the purpose of this instance.",
            "default": {},
            "examples": [
                {
                    "email": "test@email.com"
                }
            ],
            "required": [
                "email"
            ],
            "properties": {
                "email": {
                    "$id": "#/properties/filter/properties/email",
                    "type": "string",
                    "title": "The email schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": "",
                    "examples": [
                        "test@email.com"
                    ]
                }
            },
            "additionalProperties": true
        },
        "pagination-token": {
            "$id": "#/properties/pagination-token",
            "type": "string",
            "title": "The pagination-token schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "CAISoAIXXXXXX5AQgDEvQBANg6kXXXXXXXXX"
            ]
        }
    },
    "additionalProperties": true
}